{
  "consumer": {
    "name": "Sample Book Consumer"
  },
  "interactions": [
    {
      "description": "A request for product",
      "providerState": "A product exists",
      "request": {
        "matchingRules": {
          "$.path": {
            "match": "regex",
            "regex": "/BookStore/v1/Book/ISBN=9781449331818"
          }
        },
        "method": "GET",
        "path": "^/BookStore/v1/Book/ISBN=[0-9]+$"
      },
      "response": {
        "body": {
          "author": "Addy Osmani",
          "description": "With Learning JavaScript Design Patterns",
          "isbn": "9781449331818",
          "pages": 254,
          "publish_date": "2020-06-04T09:11:40.000Z",
          "publisher": "O'Reilly Media",
          "subTitle": "A JavaScript and jQuery Developer's Guide",
          "title": "Learning JavaScript Design Patterns",
          "website": "http://www.addyosmani.com.br"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "$.body.author": {
            "match": "type"
          },
          "$.body.description": {
            "match": "type"
          },
          "$.body.isbn": {
            "match": "type"
          },
          "$.body.pages": {
            "match": "type"
          },
          "$.body.publish_date": {
            "match": "type"
          },
          "$.body.publisher": {
            "match": "type"
          },
          "$.body.subTitle": {
            "match": "type"
          },
          "$.body.title": {
            "match": "type"
          },
          "$.body.website": {
            "match": "type"
          }
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-go": {
      "version": "2.3.0"
    },
    "pactRust": {
      "ffi": "0.4.28",
      "mockserver": "1.2.11",
      "models": "1.2.8"
    },
    "pactSpecification": {
      "version": "2.0.0"
    }
  },
  "provider": {
    "name": "Sample Book Provider"
  }
}